<Window x:Class="DemoPraktika.Windows.ModWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoPraktika.Windows"
        mc:Ignorable="d"
        Title="ModWindow" Height="450" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="24*"></RowDefinition>
            <RowDefinition Height="193*"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal" >
            <TextBlock x:Name="tbWinName" Text="Модератор" VerticalAlignment="Center" 
                       Margin="100,0,0,0" FontSize="14" FontWeight="Bold"></TextBlock>
            <Button x:Name="btnBack" Content="Назад" Width="180" Height="25" Margin="60,0,0,0" Click="BtnGuest_Click" ></Button>
        </StackPanel>
    </Grid>
</Window>
